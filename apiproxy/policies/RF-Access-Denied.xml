<RaiseFault name='RF-Access-Denied'>
  <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
  <FaultResponse>
    <Set>
      <Payload contentType='application/json'>{
  "error" : {
    "code" : 403.01,
    "message" : "access denied."
  }
}
</Payload>
      <StatusCode>403</StatusCode>
    </Set>
  </FaultResponse>
</RaiseFault>
